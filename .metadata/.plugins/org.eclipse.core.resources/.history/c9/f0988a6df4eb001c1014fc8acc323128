import java.util.Iterator;

public class Main {
	public static void main(String[] args) {
		
		/*
		 * APARTADO 1
		 */
		int j = 0;
		while (j <= 3 ) {
			System.out.println(j);
			j++;
			break;
		}
		
		/*
		 * APARTADO 2
		 */
		String text = "IteraciÃ³n";
		int numero = 100;
		
		for (int i = 0; i < 5; i++) {
			System.out.println(text+ " "+numero);
		}
	}
}
