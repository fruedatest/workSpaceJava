public class Main {
	public static void main(String[] args) {
		
		/*
		 * APARTADO 1
		 */
		
		for (int i = 0; i <= 3; i++) {
			System.out.println(i);
		}
		
		/*
		 * APARTADO 2
		 */
		String text = "IteraciÃ³n";
		int numero = 100;
		
		for (int i = 0; i < 5; i++) {
			System.out.println(text+ " "+numero);
		}
	}
}
