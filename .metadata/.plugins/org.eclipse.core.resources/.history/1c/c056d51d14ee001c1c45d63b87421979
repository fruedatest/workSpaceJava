import java.util.Scanner;

public class Main {
	public static void main(String[] args) {
		
		String opcion = "";
		Scanner scanner = new Scanner(System.in);
		opcion = scanner.next().toLowerCase();

		if (opcion == "federico") {
			System.out.println("1");
			System.out.println(opcion);
		} else {
			System.out.println("2");
			System.out.println(opcion);

		}
	}

}
