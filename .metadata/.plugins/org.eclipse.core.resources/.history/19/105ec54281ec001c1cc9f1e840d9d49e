public class Main {
	public static void main(String[] args) {
		
		boolean isBoolean = true;
		char isCharacter = 'A';
		float isFloat = 12345;
		double isDouble = 1234.567;
		byte isByte = 100;
		short isShort = (short) 123456;
		int isInt = 123456;
		long isLong = 123456789123456789L;
		
		
		final boolean ISBOOLEANCONST = isBoolean;
		final char ISCHARACTERCONST = isCharacter;
		final float ISFLOATCONST = isFloat;
		final double ISDOUBLECONST = isDouble;
		final byte ISBYTECONST = isByte;
		final short ISSHORTCONST = isShort;
		final int ISINTCONST = isInt;
		final long ISLONGCONST = isLong;
		
		System.out.println(ISBOOLEANCONST);
		
	}
}
